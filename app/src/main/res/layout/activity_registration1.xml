<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txtAddress"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img"
    tools:context=".RegistrationActivity1">

    <View
        android:id="@+id/top_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/bg_views"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <GridLayout
        android:id="@+id/gridLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/top_view"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:padding="7dp"
        android:text="PROFESSEUR"
        android:textColor="#0D0E0E"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="158dp"
        android:padding="7dp"
        android:text="APP."
        android:textColor="#0F1112"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="124dp"
        android:layout_marginTop="48dp"
        android:layout_marginBottom="51dp"
        app:layout_constraintBottom_toTopOf="@+id/mStreet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        app:srcCompat="@drawable/img_8" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="14dp"
        android:text="ADDRESS;"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/mStreet"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_view" />

    <EditText
        android:id="@+id/mStreet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:hint="Street Line Address 1"
        android:inputType="text"
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mStreet2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <EditText
        android:id="@+id/mStreet2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:ems="10"
        android:hint="Street Line Address 2"
        android:inputType="text"
        android:text=""
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mCity"
        app:layout_constraintStart_toStartOf="@+id/mStreet"
        app:layout_constraintTop_toBottomOf="@+id/mStreet" />

    <Button
        android:id="@+id/mbtnproceed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:text="PROCEED"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mCountry" />

    <EditText
        android:id="@+id/mCity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:hint="CITY"
        android:inputType="text"
        android:text=""
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mCountry"
        app:layout_constraintStart_toStartOf="@+id/mStreet2"
        app:layout_constraintTop_toBottomOf="@+id/mStreet2" />

    <EditText
        android:id="@+id/mCountry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="45dp"
        android:ems="10"
        android:hint="COUNTRY"
        android:inputType="text"
        android:text=""
        android:textColor="@color/black"
        android:textSize="23sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toTopOf="@+id/mbtnproceed"
        app:layout_constraintStart_toStartOf="@+id/mCity"
        app:layout_constraintTop_toBottomOf="@+id/mCity" />


</androidx.constraintlayout.widget.ConstraintLayout>